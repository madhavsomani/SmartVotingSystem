<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hnt.smartvotingsystem.smartvotingsystem">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.USB_PERMISSION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".vote" android:theme="@style/AppTheme2"></activity>
        <activity android:name=".register"></activity>
        <activity android:name=".fingerscanregister"></activity>
        <activity android:name=".fingerscanverification"></activity>
        <activity android:name=".partydiscription" android:theme="@style/AppTheme2"></activity>
        <activity android:name=".profile" android:theme="@style/AppTheme2"></activity>
    </application>

</manifest>